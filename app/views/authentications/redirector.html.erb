<%= javascript_tag do -%>
	// If we were opened as a popup, redirect the opener (so the user is signed in) and close ourselves
	// otherwise, just redirect ourselves
	if(window.opener){
		window.opener.location = "<%= @opener_location %>";
		window.close();
	}
	else {
		window.location = "<%= @opener_location %>";
	}
<% end -%>